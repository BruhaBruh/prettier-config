{
  "name": "@bruhabruh/prettier-config",
  "version": "1.1.5",
  "description": "BruhaBruh Prettier Config for projects",
  "author": "BruhaBruh",
  "license": "MIT",
  "keywords": [
    "prettier",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/prettier-config"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "prettier --check '**/*.{js,md,yaml,yml,json}'",
    "prettier": "prettier '**/*.{js,md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "prettier": "*"
  },
  "nano-staged": {
    "*.{js,md,yaml,yml,json}": [
      "prettier --write"
    ]
  }
}
